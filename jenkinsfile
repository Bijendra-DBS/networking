pipeline {
  agent any
  stages {
    stage('Install') {
      steps { sh 'npm install' }
    }

    stage('Test') {
      parallel {
        stage('Static code analysis') {
            steps { echo 'Static code run! ' }
        }
        stage('Unit tests') {
            steps { echo 'Unit test code run! ' }
        }
      }
    }

    stage('Build') {
      steps { echo 'Build code run!' }
    }
  }
}
